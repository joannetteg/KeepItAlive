; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="LD46-KeepItAlive"
run/main_scene="res://Scene/Main.tscn"
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.164706, 0, 0, 1 )
config/icon="res://Resources/Mockups/ICN_KeepKittyAlive.png"

[display]

window/size/width=256
window/size/height=144
window/size/resizable=false
window/size/test_width=1024
window/size/test_height=576
window/per_pixel_transparency/allowed=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[rendering]

quality/driver/driver_name="GLES2"
quality/intended_usage/framebuffer_allocation=0
vram_compression/import_etc=true
vram_compression/import_etc2=false
quality/directional_shadow/size=1024
quality/shadow_atlas/size=1024
environment/default_clear_color=Color( 0.164706, 0, 0, 1 )
quality/reflections/atlas_size=1024
quality/depth/hdr=false
environment/default_environment="res://default_env.tres"
